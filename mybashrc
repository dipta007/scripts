# I generally keep all my scripts in ~/me/bin. So I need to add the path to PATH.
PATH="$PATH:$HOME/me/bin"

# Alias
alias firefox="firefox </dev/null >/dev/null 2>&1"
alias chrome="google-chrome-stable </dev/null >/dev/null 2>&1"
alias system-update="sudo apt-get update -y && sudo apt-get upgrade -y && sudo apt-get autoremove -y"

# Clear Command Line
clear
